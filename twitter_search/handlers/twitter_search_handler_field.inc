<?php
/**
 * @file
 *   Views field handler for basic Twitter tweet text fields.
 */

/**
 * Views field handler for basic Twitter tweet text fields.
 *
 * The only thing we're doing here is making sure the field_alias
 * gets set properly, and that none of the sql-specific query functionality
 * gets called.
 *
 * Basically there's a lot of table-specific functionality happening
 * in query() and this just stops that.
 *
 */
class twitter_search_handler_field extends views_handler_field {
  function query() {
    $this->field_alias = $this->real_field;
  }
}
